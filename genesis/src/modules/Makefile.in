# ColdX driver

CFLAGS=@CFLAGS@ -I../include
LDFLAGS=@LDFLAGS@
LIBS=@LIBS@
CC=@CC@
RANLIB=@RANLIB@
LINT=@LINT@
YACC=@YACC@

SYS=@SYS@
CPU=@CPU@
VENDOR=@VENDOR@

prefix=@prefix@
BINDIR=$(prefix)/bin
SRCDIR=$(prefix)/src
MODULES=$(SRCDIR)/modules

VERSION=@VERSION@

CDC_OBJS = cdc.o cdc_buffer.o cdc_misc.o cdc_dict.o cdc_list.o cdc_string.o
COLDCORE_O = core.o core_veil.o
WEB_OBJS = web.o

SRCS = `echo $(CDC_OBJS) $(COLDCORE_O) | sed -e 's/\.o/.c/g'`

#######
# rules

libs: libcdc.a libcore.a libweb.a

libweb.a: $(WEB_OBJS)
	rm -f $@
	ar -cq $@ $(WEB_OBJS)
	$(RANLIB) $@

libcdc.a: $(CDC_OBJS)
	rm -f $@
	ar -cq $@ $(CDC_OBJS)
	$(RANLIB) $@

libcore.a: $(COLDCORE_O)
	rm -f $@
	ar -cq $@ $(COLDCORE_O)
	$(RANLIB) $@

clean:
	rm -f lib*.a *~ *.BAK *.bak *.o

scrub: clean

makefile:
	@cd $(prefix); configure -q

install: libs

depend:
	@makedepend -- $(CFLAGS) -- $(SRCS)
	@rm Makefile.bak

lint:
	$(LINT) -u $(SRCS)

patchable: scrub
	@chmod -f u+w Makefile
	@echo "all:" > Makefile
	@echo "	@echo \"** Invalid Makefile! **\"" >> Makefile
	@echo "	@echo \"** running the configure script ... **\"" >> Makefile
	@echo "	@cd ../..; configure" >> Makefile

../include/parse.h:
	cd ../; make grammar.o include/parse.h

